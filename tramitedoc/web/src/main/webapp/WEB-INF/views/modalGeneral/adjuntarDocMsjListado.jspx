<jsp:root xmlns:tiles="http://tiles.apache.org/tags-tiles" version="2.0"           xmlns:c="http://java.sun.com/jsp/jstl/core" xmlns:jsp="http://java.sun.com/JSP/Page"           xmlns:spring="http://www.springframework.org/tags"           xmlns:fn="http://java.sun.com/jsp/jstl/functions"           xmlns:form="http://www.springframework.org/tags/form">    <jsp:directive.page contentType="text/html;charset=UTF-8"/>    <jsp:output omit-xml-declaration="yes"/>    <spring:eval expression="@applicationProps['application.version']" var="applicationVersion" scope="request"/>    <c:set var="rutactx" value="${pageContext.request.contextPath}/${applicationVersion}" scope="request" /><!--    <div id="btn-defecto" style="visibility: hidden;display: none;">        <span id="btn-ReemplazarDocIE" class="btn btn-default btn-file" style="width: 4px; height: 11px;">            <span class="glyphicon glyphicon-transfer"></span><input id="fileuploadReIE" type="file" name="files[]" data-url="" multiple="" style="width: 28px; height: 23px; top: -26px;right: 12px;"/>        </span>              <button id="btn-firmar" title="Firmar documento" class="btn btn-success btn-xs" onclick="fn_firmarDocumentoAnexo(this);" type="button"><span class="glyphicon glyphicon-pencil"></span></button>            </div>-->    <div class="ui-datatable ui-datatable-scrollable">        <div class="ui-datatable-scrollable-header">            <table class="ui-state-default" style="width:100%;">                <thead>                    <tr class="ui-state-default">                        <th width="25%">Nombre Doc.</th>                        <th width="30%">Observaci√≥n</th>                        <th width="17%">Fecha Carga</th>                        <th width="8%">Usuario</th>                        <th width="10%">Tipo Doc.</th>                        <th width="10%">Opciones</th>                    </tr>                </thead>            </table>        </div>        <div class="bx_sb ui-datatable-scrollable-body" style='height:115px;'>            <table class="ui-datatable-edit-data" id="tblDocAnexos" cellpadding="0" cellspacing="0" style="width:100%;">                <tbody>                    <c:forEach var="refDocAdm" items="${docAnexoListMsj}" varStatus="status">                        <tr class="gradeA" tabindex="0">                            <td width="25%">${refDocAdm.deDet}</td>                            <td width="30%">${refDocAdm.obMsj}</td>                            <td width="17%">${refDocAdm.feUseCre}</td>                            <td width="8%">${refDocAdm.coUseCre}</td>                             <td width="10%">${refDocAdm.deDocMsj}</td>                            <td width="10%" align="center">                                <button title ="Ver Documento" class="btn btn-default btn-xs" onclick="fn_verDocMsjAdicional('${refDocAdm.nuAnn}', '${refDocAdm.nuEmi}', '${refDocAdm.nuDes}', '${refDocAdm.nuAne}');" nuAnn="${refDocAdm.nuAnn}" nuEmi="${refDocAdm.nuEmi}" nuDes="${refDocAdm.nuDes}" type="button">                                    <span class="glyphicon glyphicon-folder-open"/>                                </button>                                 <c:choose>                                    <c:when test='${refDocAdm.inDifHora eq 1}'>                                        <button id="btn-eliminar" title="Eliminar documento" class="btn btn-default btn-xs"                                                 onclick="fn_eliminarAdjuntarDoc('${refDocAdm.nuAnn}', '${refDocAdm.nuEmi}', '${refDocAdm.nuDes}', '${refDocAdm.nuAne}');"                                                 type="button">                                            <span class="glyphicon glyphicon-trash"></span>                                        </button>                                    </c:when>                                </c:choose>                            </td>                        </tr>                    </c:forEach>                </tbody>	            </table>        </div>                        </div>                                              <div>        <input id="fileupload" type="file" name="files[]" data-url="" multiple="" style="display: none;"/>        <div id="progress" class="progress" style="margin-top:15px;width:200px;display: none;">            <div class="progress-bar" role="progressbar" aria-valuenow="0" aria-valuemin="0" aria-valuemax="100" style="width: 0%;">                <span style="top: 3px; "><jsp:text/></span>            </div>        </div>    </div>                      </jsp:root>