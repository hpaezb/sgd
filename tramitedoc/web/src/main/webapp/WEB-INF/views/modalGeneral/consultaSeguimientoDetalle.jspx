<jsp:root xmlns:tiles="http://tiles.apache.org/tags-tiles" version="2.0"           xmlns:c="http://java.sun.com/jsp/jstl/core"           xmlns:jsp="http://java.sun.com/JSP/Page"           xmlns:spring="http://www.springframework.org/tags"           xmlns:fn="http://java.sun.com/jsp/jstl/functions"           xmlns:form="http://www.springframework.org/tags/form">    <jsp:directive.page contentType="text/html;charset=UTF-8"/>    <jsp:output omit-xml-declaration="yes"/>       <input type="hidden" id="idMensajeria" value="${docMensajeriaBean.numsj}"/>     <input type="hidden" id="cantExpEnlazados" value="${cantExpEnl}"/> <!--Hermes Enlace de expedientes 17/08/2018-->    <input type="hidden" id="idMensajeriaDevolucion" value="${docRemitoBean.docEstadoMsj}"/><!--22/08/19 HPB Devolucion Doc a Oficina-->     <input type="hidden" id="indMesaVirtual" value="${indMesaVirtual}"/><!--HPB 05/12/2019 - Requerimiento MPV-OBSERVADOS-->     <input type="hidden" id="pCoEmpEmi2" value="${pkCoEmpEmi}"/> <!--/*[HPB-02/10/20] Inicio - Plazo de Atencion-*/-->    <input type="hidden" id="pCoEmpDes" value="${docDestinoBean.coEmpDes}"/> <!--/*[HPB-02/10/20] Inicio - Plazo de Atencion-*/-->    <input type="hidden" id="esDocRec" value="${docDestinoBean.esDocRec}"/> <!--/*[HPB-02/10/20] Inicio - Plazo de Atencion-*/-->    <fieldset>        <legend>Remitente</legend>        <table width="100%">            <tbody>                <tr>                    <td style="width:70px;">N° Exp.:</td>                    <td><input id="txtNuExpediente" style="width: 200px;" type="text" readonly="readonly" value="${docRemitoBean.nuExpediente}" ok=""/></td>                    <td style="width:70px;">Fecha:</td>                    <td></td>                </tr>                <tr>                    <td style="width:70px;">Tipo Doc.:</td>                    <td><input id="txtTipoDocumento" style="width: 200px;" type="text" readonly="readonly" value="${docRemitoBean.deTiDoc}" ok=""/></td>                    <td style="width:70px;">Nro Doc.:</td>                    <td><input id="txtNroDocumento" type="text" style="width: 300px;" readonly="readonly" value="${docRemitoBean.nuDoc}"/></td>                </tr>                <tr>                    <td>Fecha Emi.:</td>                    <td><input id="txtFechaEmsion" style="text-align: left; width: 200px;" type="text" readonly="readonly" value="${docRemitoBean.feEmi}" ok=""/></td>                    <td>Elaboró:</td>                    <td><input id="txtElavorado" style="width: 300px;" type="text" readonly="readonly" value="${docRemitoBean.deEmpRes}"/></td>                </tr>                <tr>                    <td>Emisor:</td>                    <td colspan="3"><input id="txtDependenciaPersonal" style="width: 578px;" type="text" readonly="readonly" value="${docRemitoBean.deOriEmi}"/></td>                </tr>                <tr>                    <td>Asunto:</td>                    <td colspan="3"><textarea id="txtAsunto" cols="70" style="width: 578px; height: 41px;" rows="4" readonly="readonly">${docRemitoBean.deAsu}</textarea></td>                </tr>                <!-- [HPB] Inicio 24/02/23 CLS-087-2022 -->                <tr>                    <td>Dirección</td>                    <td colspan="5"><input id="txtDireccion" style="width: 579px;" type="text" readonly="readonly" value="${docRemitoBean.deDireccion}"/></td>                </tr>                                <tr>                    <td>Teléfono</td>                    <td><input id="txtTelefono" style="width: 180px;" type="text" readonly="readonly" value="${docRemitoBean.telefono}"/></td>                    <td>Correo:</td>                    <td><input id="txtCorreo" style="text-align: left; width: 300px;" type="text" readonly="readonly" value="${docRemitoBean.deCorreo}" ok=""/></td>                </tr>                               <!-- [HPB] Fin 24/02/23 CLS-087-2022 -->                <tr>                    <td>Estado:</td>                    <td><input id="txtEstado" type="text" style="width: 180px;" readonly="readonly" value="${docRemitoBean.deEsDocEmi}"/></td>                    <td colspan="2" align="center" >                        <div class="btn-group">                            <!--[HPB-02/10/20] Inicio - Plazo de Atencion-->                            <c:choose>                                <c:when test="${docDestinoBean.coEmpDes eq pkCoEmpEmi and docDestinoBean.deEsDocDes eq 'NO LEIDO'}"><!--                                    <button title ="Recepcionar" class="btn btn-default btn-sm" onclick="editarDocumentoRecepClickPlazoAtencion('${docRemitoBean.nuAnn}', '${docRemitoBean.nuEmi}','${docDestinoBean.nuDes}');" type="button">                                        <span class="glyphicon glyphicon-import"/> Recibir                                    </button>  -->                                </c:when>                            </c:choose>                             <!--[HPB-02/10/20] Inicio - Plazo de Atencion-->                                                       <button title ="Ver Documento" class="btn btn-default btn-sm" onclick="fn_verDocumento('${docRemitoBean.nuAnn}', '${docRemitoBean.nuEmi}');" type="button">                                <span class="glyphicon glyphicon-folder-open"/> Abrir Documento                            </button>                            <button title ="Abrir anexos" class="btn btn-default btn-sm" onclick="fn_verAnexo('${docRemitoBean.nuAnn}', '${docRemitoBean.nuEmi}', '${docDestinoBean.nuDes}');" type="button">                                <span class="glyphicon glyphicon-paperclip"/> Documentos anexos                            </button>                            <!--Enlace Expedientes - Hermes 17/09/2018-->                            <div id="divEnlaceExpediente">                                <button title ="Ver Expedientes enlazados" class="btn btn-default btn-sm" onclick="fn_lstEnlaceExpediente('${docRemitoBean.nuAnn}', '${docRemitoBean.nuEmi}');" type="button">                                    <span class="glyphicon glyphicon-paperclip"/> Ver Rpta.                                </button>                               </div>                            <!--Enlace Expedientes-->                        </div>                    </td>                </tr>            </tbody>        </table>    </fieldset>     <fieldset>        <legend>Destinatario</legend>        <table width="100%">            <tbody>                <tr>                    <td>Dependencia:</td>                    <td colspan="5"><input id="txtDependencia" style="width: 579px;" type="text" readonly="readonly" value="${docDestinoBean.deOriDes}"/></td>                </tr>                <tr>                    <td>Receptor:</td>                    <td colspan="5"><input id="txtReceptor" style="width: 579px;" type="text" readonly="readonly" value="${docDestinoBean.deEmpRec}"/></td>                </tr>                <tr>                    <td>Estado:</td>                    <td><input id="txtEstado" type="text" style="width: 135px;" readonly="readonly" value="${docDestinoBean.deEsDocDes}"/></td>                    <td>Fecha Rec.:</td>                    <td><input id="txtFechaRecepcion" type="text" style="text-align: center; width: 125px;" readonly="readonly" value="${docDestinoBean.feRecDoc}"/></td>                    <td>Fecha Ate.:</td>                    <td><input id="txtFechaAtencion" type="text" style="text-align: center; width: 171px;" readonly="readonly" value="${docDestinoBean.feAteDoc}"/></td>                </tr>                <tr>                    <td>Trámite:</td>                    <td><input id="txtMotivo" type="text" style="width: 135px;" readonly="readonly" value="${docDestinoBean.deMot}"/></td>                    <td>Prioridad:</td>                    <td><input id="txtPrioridad" type="text" style="width: 125px;" readonly="readonly" value="${docDestinoBean.dePri}"/></td>                    <td>Indicaciones:</td>                    <td><input id="txtPrioridad" type="text" style="width: 171px;" readonly="readonly" value="${docDestinoBean.deIndica}"/></td>                                    </tr>                <c:if test="${not empty docDestinoBean.deAne and fn:length(docDestinoBean.deAne.trim())>0 }">                    <tr>                        <td>Obs:</td>                        <td colspan="5">                            <textarea id="txtAsunto" cols="70" style="width: 570px; height: 41px;" rows="4" readonly="readonly">${docDestinoBean.deAne}</textarea>                        </td>                    </tr>                </c:if>                <!-- [HPB] Inicio 27/03/23 CLS-087-2022 -->                <tr>                    <td>Fecha Observación:</td>                    <td><input id="txtFechaEnvCorObs" style="text-align: left; width: 135px;" type="text" readonly="readonly" value="${docDestinoBean.feEnvCorObs}" ok=""/></td>                                                   </tr>                <!-- [HPB] Fin 27/03/23 CLS-087-2022 -->                 <c:if test="${not empty docDestinoBean.deAvance and fn:length(docDestinoBean.deAvance.trim())>0 }">                    <tr>                        <td>Avance:</td>                        <td colspan="4">                            <textarea id="txtAvance" cols="70" style="width: 390px; height: 41px;" rows="4" readonly="readonly">${docDestinoBean.deAvance}</textarea>                        </td>                        <td  >                            <div class="btn-group"  >                            <button title ="Ver Detalle Avance" class="btn btn-default btn-sm" onclick="fn_verAvanceConsultaObj('${docRemitoBean.nuAnn}', '${docRemitoBean.nuEmi}', '${docDestinoBean.nuDes}');" type="button">                                <span class="glyphicon glyphicon-th-list"/> Ver Detalle de Avance                            </button>                                                    </div>                                                    </td>                    </tr>                </c:if>            </tbody>        </table>    </fieldset><!--22/08/19 HPB Devolucion Doc a Oficina -->                                                                                                                                          <div id="detalleMsjDevolucion">    <fieldset>        <legend>Mensajería</legend>        <table width="100%">            <tbody>                <tr>                    <td style="width:70px;">Estado:</td>                    <td><input id="txtEstadoDev" type="text" style="width: 135px;" readonly="readonly" value="${docRemitoBean.deDocEstadoMsj}"/></td>                    <td>Fecha Dev.:</td>                    <td><input id="txtFechaDev" type="text" style="text-align: left; width: 125px;" readonly="readonly" value="${docRemitoBean.feDevOfi}"/></td>                    <td>Usuario:</td>                    <td><input id="txtUsuarioDev" type="text" style="text-align: left; width: 171px;" readonly="readonly" value="${docRemitoBean.coEmpDev}"/></td>                </tr>                 <tr>                    <td style="width:70px;">Observación:</td>                    <td colspan="5"><textarea id="txtObservacionDev" cols="70" style="width: 579px; height: 70px;" rows="4" readonly="readonly">${docRemitoBean.obsDev}</textarea></td>                </tr>                            </tbody>        </table>                                                                                                                                        </fieldset>                 </div> <!--22/08/19 HPB Devolucion Doc a Oficina -->    <!--HPB 05/12/2019 - Requerimiento MPV-OBSERVADOS -->                                                                                                                                          <div id="detalleMsjObservadoMPV">    <fieldset>        <legend>Mesa Partes Virtual</legend>        <table width="100%">            <tbody>                <tr>                    <td style="width:70px;">Estado:</td>                    <td><input id="txtEstadoDev" type="text" style="width: 135px;" readonly="readonly" value="${docRemitoBean.deDocEstadoMsj}"/></td>                </tr>             </tbody>        </table>         <br/>        <div class="ui-datatable-fixed">            <table id ="tblAnexosDetalleNew" class="display" width="100%">                <thead>                    <tr class="ui-state-default">                        <th width="140px;">Nombre Doc.</th>                        <th width="140px;">Observación</th>                        <th width="82px;">Fecha Carga</th>                        <th width="60px;">Usuario</th>                        <th width="24px;">Tipo Doc.</th>                        <th width="26px;">Doc. Adjunto</th>                    </tr>                </thead>				                <tbody>                    <c:forEach var="refDocAdm" items="${docAnexoListMsj}" varStatus="status">                        <tr class="gradeA" tabindex="0">                            <td>${refDocAdm.deDet}</td>                            <td>${refDocAdm.obMsj}</td>                            <td>${refDocAdm.feUseCre}</td>                            <td>${refDocAdm.coUseCre}</td>                             <td>${refDocAdm.deDocMsj}</td>                            <td align="center">                                <button title ="Ver Documento" class="btn btn-default btn-xs" onclick="fn_verDocMsjAdicional('${refDocAdm.nuAnn}', '${refDocAdm.nuEmi}', '${refDocAdm.nuDes}', '${refDocAdm.nuAne}');" nuAnn="${refDocAdm.nuAnn}" nuEmi="${refDocAdm.nuEmi}" nuDes="${refDocAdm.nuDes}" type="button">                                    <span class="glyphicon glyphicon-folder-open"/>                                </button>                            </td>                        </tr>                    </c:forEach>                </tbody>			            </table>          </div>        <div id="divError" class="alert alert-danger" style="display:none">            <ul id="divErrorLista"><jsp:text/></ul>        </div>                      </fieldset>                 </div>             <div id="detalleDevolucionMPV">    <fieldset>        <legend>Mesa Partes Virtual</legend>        <table width="100%">            <tbody>                <tr>                    <td style="width:70px;">Estado:</td>                    <td><input id="txtEstadoDev" type="text" style="width: 135px;" readonly="readonly" value="${docRemitoBean.deDocEstadoMsj}"/></td>                                        <td>Fecha Devolución:</td>                    <td><input id="txtFechaEmsion" style="text-align: left; width: 200px;" type="text" readonly="readonly" value="${docRemitoBean.feDevOfi}" ok=""/></td>                </tr>                <tr>                    <td>Observación:</td>                    <td colspan="3"><textarea id="txtAsunto" cols="70" style="width: 578px; height: 41px;" rows="4" readonly="readonly">${docRemitoBean.obsMpv}</textarea></td>                </tr>                            </tbody>        </table>         <br/>                    </fieldset>                 </div>         <!--HPB 05/12/2019 - Requerimiento MPV-OBSERVADOS -->       <div id="detalleMsj">     <fieldset >        <legend>Mensajería</legend>        <table width="100%">            <tbody>                <tr>                    <td>Asignado a:</td>                    <td><input id="txtTipoMensajero" style="width: 135px;" type="text" readonly="readonly" value="${docMensajeriaBean.detipmsj}" ok=""/></td>                    <td>Resp. Env.:</td>                    <td><input id="txtResponsableEnvio" type="text" style="width: 125px;" readonly="readonly" value="${docMensajeriaBean.reenvmsj}"/></td>                    <td>Estado:</td>                    <td><input id="txtTipoMensajero" style="width: 171px;" type="text" readonly="readonly" value="${docMensajeriaBean.docEstadoMsj}" ok=""/></td>                                    </tr>                <tr>                    <td>Envío a Msjro.:</td>                    <td><input id="txtFechaEnvio" style="text-align: left; width: 135px;" type="text" readonly="readonly" value="${docMensajeriaBean.fecEnviomsj}"/></td>                    <td>Tipo Env.:</td>                    <td><input id="txtTipoEnvio" style="width: 125px;" type="text" readonly="readonly" value="${docMensajeriaBean.deTipEnv}"/></td>                    <td>Motivo:</td>                    <td><input id="txtFechaEnvio" style="text-align: left; width: 171px;" type="text" readonly="readonly" value="${docMensajeriaBean.moMsjDev}"/></td>                </tr>                <tr>                    <td>Ent. a Dest.:</td>                    <td><input id="txtEstado" type="text" style="width: 135px;" readonly="readonly" value="${docMensajeriaBean.feEntMsj}"/></td>                    <td>Ámbito:</td>                    <td><input id="txtResponsableEnvio" type="text" style="width: 125px;" readonly="readonly" value="${docMensajeriaBean.deambito}"/></td>                    <td>Observación:</td>                    <td rowspan="2"><textarea id="txtAsunto" cols="70" style="width: 171px; height: 55px;" rows="4" readonly="readonly">${docMensajeriaBean.obMsj}</textarea></td>                </tr>                   <tr>                    <td>Devol. Cargo:</td>                    <td><input id="txtTipoMensajero" style="width: 135px;" type="text" readonly="readonly" value="${docMensajeriaBean.feDevMsj}" ok=""/></td>                    <td>Tipo Zona:</td>                    <td><input id="txtResponsableEnvio" type="text" style="width: 125px;" readonly="readonly" value="${docMensajeriaBean.tiZona}"/></td>                                  </tr>                  <tr>                    <td colspan="2" align="left" >                        <div class="btn-group">                            <button title ="Ver Cargo y Documento(s) Adicional(es)" class="btn btn-default btn-sm" onclick="fn_goLstDocAdicionalesMsj('${docRemitoBean.nuAnn}', '${docRemitoBean.nuEmi}', '${docDestinoBean.nuDes}');" type="button">                                <span class="glyphicon glyphicon-folder-open"/> Ver cargos                            </button>                        </div>                    </td>                                </tr>                             </tbody>        </table>        <br/>            <!--Hermes 22/01/2019 - Requerimiento Acta 005-20019 -->                                                    <div class="ui-datatable-fixed" style="display: none;">            <table id ="tblAnexosDetalle1" class="display" width="100%">                <thead>                    <tr class="ui-state-default">                        <th width="140px;">Nombre Doc.</th>                        <th width="140px;">Observación</th>                        <th width="82px;">Fecha Carga</th>                        <th width="60px;">Usuario</th>                        <th width="24px;">Tipo Doc.</th>                        <th width="26px;">Doc. Adjunto</th>                    </tr>                </thead>				                <tbody>                    <c:forEach var="refDocAdm" items="${docAnexoListMsj}" varStatus="status">                        <tr class="gradeA" tabindex="0">                            <td>${refDocAdm.deDet}</td>                            <td>${refDocAdm.obMsj}</td>                            <td>${refDocAdm.feUseCre}</td>                            <td>${refDocAdm.coUseCre}</td>                            <td>${refDocAdm.deDocMsj}</td>                            <td align="center">                                <button title ="Ver Documento" class="btn btn-default btn-xs" onclick="fn_verDocMsjAdicional('${refDocAdm.nuAnn}', '${refDocAdm.nuEmi}', '${refDocAdm.nuDes}', '${refDocAdm.nuAne}');" nuAnn="${refDocAdm.nuAnn}" nuEmi="${refDocAdm.nuEmi}" nuDes="${refDocAdm.nuDes}" type="button">                                    <span class="glyphicon glyphicon-folder-open"/>                                </button>                            </td>                        </tr>                    </c:forEach>                </tbody>			            </table>                  </div>        <!--Hermes 22/01/2019 - Requerimiento Acta 005-20019 -->                                                                                                </fieldset>                 </div>            <script>        fu_callEventoTablaAnexosdetalleAdicional();<!--Hermes 22/01/2019 - Requerimiento Acta 005-20019 -->        $("#windowConsultaSeguimiento").show('fast');        jQuery(".tree ul.dynatree-container").css('width', '100%').css('width', '-=20px');        jQuery(".tree ul.dynatree-container").css('height', '100%').css('height', '-=20px');                      <!--Hermes 22/01/2019 - Requerimiento Acta 005-20019 -->            if($('#idMensajeriaDevolucion').val()==='6'){                $('#detalleMsjDevolucion').show();            }else{                $('#detalleMsjDevolucion').hide();                            }            <!--Hermes 22/01/2019 - Requerimiento Acta 005-20019 -->            <!--HPB 05/12/2019 - Requerimiento MPV-OBSERVADOS -->            if($('#idMensajeriaDevolucion').val()==='7' || $('#indMesaVirtual').val()==='2'){                $('#detalleMsjObservadoMPV').show();            }else{                $('#detalleMsjObservadoMPV').hide();                            }               if($('#idMensajeriaDevolucion').val()==='8'){                $('#detalleDevolucionMPV').show();            }else{                $('#detalleDevolucionMPV').hide();                            }             <!--HPB 05/12/2019 - Requerimiento MPV-OBSERVADOS -->                        if($('#idMensajeria').val()==='0'){            $('#detalleMsj').hide();            }            else            {                $('#detalleMsj').show();                            }            <!--Enlace Expedientes - Hermes 17/09/2018-->            if($('#cantExpEnlazados').val()==='0'){                $('#divEnlaceExpediente').hide();            }else{                $('#divEnlaceExpediente').show();            }        </script> </jsp:root>