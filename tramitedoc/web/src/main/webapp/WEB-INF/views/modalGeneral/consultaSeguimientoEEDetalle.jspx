<jsp:root xmlns:tiles="http://tiles.apache.org/tags-tiles" version="2.0" xmlns:c="http://java.sun.com/jsp/jstl/core" xmlns:jsp="http://java.sun.com/JSP/Page" xmlns:spring="http://www.springframework.org/tags" xmlns:fn="http://java.sun.com/jsp/jstl/functions" xmlns:form="http://www.springframework.org/tags/form">    <jsp:directive.page contentType="text/html;charset=UTF-8"/>    <jsp:output omit-xml-declaration="yes"/>       <input type="hidden" id="idMensajeria" value="${docMensajeriaBean.numsj}"/>     <fieldset>        <legend>Remitente</legend>        <table width="100%">            <tbody>                <tr>                    <td style="width:70px;">N° Exp.:</td>                    <td><input id="txtNuExpediente" style="width: 200px;" type="text" readonly="readonly" value="${docRemitoBean.nuExpediente}" ok=""/></td>                    <td style="width:70px;">Fecha:</td>                    <td></td>                </tr>                <tr>                    <td style="width:70px;">Tipo Doc.:</td>                    <td><input id="txtTipoDocumento" style="width: 200px;" type="text" readonly="readonly" value="${docRemitoBean.deTiDoc}" ok=""/></td>                    <td style="width:70px;">Nro Doc.:</td>                    <td><input id="txtNroDocumento" type="text" style="width: 300px;" readonly="readonly" value="${docRemitoBean.nuDoc}"/></td>                </tr>                <tr>                    <td>Fecha Emi.:</td>                    <td><input id="txtFechaEmsion" style="text-align: left; width: 200px;" type="text" readonly="readonly" value="${docRemitoBean.feEmi}" ok=""/></td>                    <td>Elaboró:</td>                    <td><input id="txtElavorado" style="width: 300px;" type="text" readonly="readonly" value="${docRemitoBean.deEmpRes}"/></td>                </tr>                <tr>                    <td>Emisor:</td>                    <td colspan="3"><input id="txtDependenciaPersonal" style="width: 578px;" type="text" readonly="readonly" value="${docRemitoBean.deOriEmi}"/></td>                </tr>                <tr>                    <td>Asunto:</td>                    <td colspan="3"><textarea id="txtAsunto" cols="70" style="width: 578px; height: 41px;" rows="4" readonly="readonly">${docRemitoBean.deAsu}</textarea></td>                </tr>                <tr>                    <td>Estado:</td>                    <td><input id="txtEstado" type="text" style="width: 180px;" readonly="readonly" value="${docRemitoBean.deEsDocEmi}"/></td>                    <td colspan="2" align="center" >                        <div class="btn-group">                            <button title ="Ver Documento" class="btn btn-default btn-sm" onclick="fn_verDocumento('${docRemitoBean.nuAnn}', '${docRemitoBean.nuEmi}');" type="button">                                <span class="glyphicon glyphicon-folder-open"/> Abrir Documento                            </button>                            <button title ="Abrir anexos" class="btn btn-default btn-sm" onclick="fn_verAnexo('${docRemitoBean.nuAnn}', '${docRemitoBean.nuEmi}', '${docDestinoBean.nuDes}');" type="button">                                <span class="glyphicon glyphicon-paperclip"/> Documentos anexos                            </button>                            <!--Enlace Expedientes - Hermes 17/09/2018                            <button title ="Ver Expedientes enlazados" class="btn btn-default btn-sm" onclick="fn_verSeguimientoDocExtRecep();" type="button">                                <span class="glyphicon glyphicon-paperclip"/> Ver Rpta.                            </button>                                                            Enlace Expedientes-->                        </div>                    </td>                </tr>            </tbody>        </table>    </fieldset>     <fieldset>        <legend>Destinatario</legend>        <table width="100%">            <tbody>                <tr>                    <td>Dependencia:</td>                    <td colspan="5"><input id="txtDependencia" style="width: 579px;" type="text" readonly="readonly" value="${docDestinoBean.deOriDes}"/></td>                </tr>                <tr>                    <td>Receptor:</td>                    <td colspan="5"><input id="txtReceptor" style="width: 579px;" type="text" readonly="readonly" value="${docDestinoBean.deEmpRec}"/></td>                </tr>                <tr>                    <td>Estado:</td>                    <td><input id="txtEstado" type="text" style="width: 135px;" readonly="readonly" value="${docDestinoBean.deEsDocDes}"/></td>                    <td>Fecha Rec.:</td>                    <td><input id="txtFechaRecepcion" type="text" style="text-align: center; width: 125px;" readonly="readonly" value="${docDestinoBean.feRecDoc}"/></td>                    <td>Fecha Ate.:</td>                    <td><input id="txtFechaAtencion" type="text" style="text-align: center; width: 171px;" readonly="readonly" value="${docDestinoBean.feAteDoc}"/></td>                </tr>                <tr>                    <td>Trámite:</td>                    <td><input id="txtMotivo" type="text" style="width: 135px;" readonly="readonly" value="${docDestinoBean.deMot}"/></td>                    <td>Prioridad:</td>                    <td><input id="txtPrioridad" type="text" style="width: 125px;" readonly="readonly" value="${docDestinoBean.dePri}"/></td>                    <td>Indicaciones:</td>                    <td><input id="txtPrioridad" type="text" style="width: 171px;" readonly="readonly" value="${docDestinoBean.deIndica}"/></td>                                    </tr>                <c:if test="${not empty docDestinoBean.deAne and fn:length(docDestinoBean.deAne.trim())>0 }">                    <tr>                        <td>Obs:</td>                        <td colspan="5">                            <textarea id="txtAsunto" cols="70" style="width: 570px; height: 41px;" rows="4" readonly="readonly">${docDestinoBean.deAne}</textarea>                        </td>                    </tr>                </c:if>                <c:if test="${not empty docDestinoBean.deAvance and fn:length(docDestinoBean.deAvance.trim())>0 }">                    <tr>                        <td>Avance:</td>                        <td colspan="4">                            <textarea id="txtAvance" cols="70" style="width: 390px; height: 41px;" rows="4" readonly="readonly">${docDestinoBean.deAvance}</textarea>                        </td>                        <td  >                            <div class="btn-group"  >                            <button title ="Ver Detalle Avance" class="btn btn-default btn-sm" onclick="fn_verAvanceConsultaObj('${docRemitoBean.nuAnn}', '${docRemitoBean.nuEmi}', '${docDestinoBean.nuDes}');" type="button">                                <span class="glyphicon glyphicon-th-list"/> Ver Detalle de Avance                            </button>                                                    </div>                                                    </td>                    </tr>                </c:if>            </tbody>        </table>    </fieldset>                    <div id="detalleMsjEE">     <fieldset >        <legend>Mensajería</legend>        <table width="100%">            <tbody>                <tr>                    <td style="width:10%;">Asignado a:</td>                    <td><input id="txtTipoMensajero" style="width: 190px;" type="text" readonly="readonly" value="${docMensajeriaBean.detipmsj}" ok=""/></td>                    <td >Resp. de Envío:</td>                    <td><input id="txtResponsableEnvio" type="text" style="width: 300px;" readonly="readonly" value="${docMensajeriaBean.reenvmsj}"/></td>                </tr>                <tr>                    <td style="width:10%;">Fecha Envío:</td>                    <td><input id="txtFechaEnvio" style="text-align: left; width: 190px;" type="text" readonly="readonly" value="${docMensajeriaBean.fecEnviomsj}"/></td>                    <td>Tipo Envío:</td>                    <td><input id="txtTipoEnvio" style="width: 300px;" type="text" readonly="readonly" value="${docMensajeriaBean.deTipEnv}"/></td>                </tr>                <tr>                     <td>Estado:</td>                    <td><input id="txtEstado" type="text" style="width: 190px;" readonly="readonly" value="${docMensajeriaBean.docEstadoMsj}"/></td>                    <td>Fecha Plazo:</td>                    <!--<td><input id="txtFechaPlazo" style="width: 300px;" type="text" readonly="readonly" value="${docMensajeriaBean.fePlaMsj}"/></td>-->                    <td><input id="txtFechaPlazo" style="width: 300px;" type="text" readonly="readonly" value="${docMensajeriaBean.fePlaMsjD}"/></td>                </tr>                           </tbody>        </table>    </fieldset>                 </div>            <script>        $("#windowConsultaSeguimientoEE").show('fast');        jQuery(".tree ul.dynatree-container").css('width', '100%').css('width', '-=20px');        jQuery(".tree ul.dynatree-container").css('height', '100%').css('height', '-=20px');                      if($('#idMensajeria').val()==='0'){            $('#detalleMsjEE').hide();            }            else            {                $('#detalleMsjEE').show();                            }    </script> </jsp:root>