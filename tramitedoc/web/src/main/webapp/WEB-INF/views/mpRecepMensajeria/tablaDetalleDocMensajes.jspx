<jsp:root xmlns:tiles="http://tiles.apache.org/tags-tiles" version="2.0" xmlns:c="http://java.sun.com/jsp/jstl/core" xmlns:jsp="http://java.sun.com/JSP/Page" xmlns:spring="http://www.springframework.org/tags" xmlns:fn="http://java.sun.com/jsp/jstl/functions" xmlns:form="http://www.springframework.org/tags/form">    <jsp:directive.page contentType="text/html;charset=UTF-8"/>    <jsp:output omit-xml-declaration="yes"/>    <script languaje="javascript">jQuery(document).ready(function() {        fn_eventdocMensajeria();    });        </script>                                    <table cellpadding="0" cellspacing="0" width="100%">            <tr valign="top">                                 <td width="100%">                    <input id="txtTblDestEmiFilaWhereButton" name="txtTblDestEmiFilaWhereButton" type="hidden"/>                    <input id="txtTblDestEmiColWhereButton" name="txtTblDestEmiColWhereButton" type="hidden"/>                    <input id="txtDetalleDestiMensAmbitos" name="txtDetalleDestiMensAmbitos" type="hidden"/>                                        <input id="txtCoTipoAccesoZona" name="txtCoTipoAccesoZona" type="hidden"/>  <!--Hermes 19/09/2018-->                                      <input id="txtCoTipoAccesoZonaNew" name="txtCoTipoAccesoZonaNew" type="hidden"/>  <!--Hermes 03/04/2019-->                                                          <div class="ui-datatable ui-datatable-scrollable">                        <div class="ui-datatable-scrollable-header">                            <table class="ui-state-default" style="width:100%;">                                <thead>                                    <tr>                                        <th width="2%"><input onclick="fn_optionSelecctionDetalleMensajDestAll(this);"  type="checkbox"/></th>                                        <th width="2%">N°</th>                                        <th width="21%">Oficina Origen</th>                                        <th width="15%">Tipo Documento</th>                                        <th width="20%">Destinatario</th>                                        <th width="15%">Departamento Destino</th>                                                                            <th width="15%">Dirección Destino</th>                                                                                        <th width="5%">Tipo Zona</th>                                        <!-- VMBP - [INICIO - 15/08/2019] - Agregar prioridad -->                                        <th width="5%">Prioridad</th>                                        <!-- VMBP - [FIN - 15/08/2019] - Agregar prioridad -->                                    </tr>                                </thead>                            </table>                        </div>                        <div class="bx_sb ui-datatable-scrollable-body" style='height:200px;'>                            <table class="ui-datatable-edit-data" id="tblDocMensajeria" cellpadding="0" cellspacing="0" style="width:100%;">                                <tbody>                                                                       <c:forEach var="lDestinatario" items="${lstDestintarioDocMensajeria}" varStatus="status">                                        <tr>                                            <td style="display:none">${lDestinatario.ccodDpto}</td>                                            <td style="display:none">${lDestinatario.ccodProv}</td>                                            <td style="display:none">${lDestinatario.ccodDist}</td>                                            <td style="display:none">${lDestinatario.coServicio}</td>                                            <td style="display:none">${lDestinatario.fePlaMsj}</td>                                            <td width="2%" class="esOptionCheckDetalle" ><input onclick="fn_optionSelecctionDetalleMensajDest();"   type="checkbox" ambito="${lDestinatario.ambito}" nuAnn="${lDestinatario.nuAnn}" nuEmi="${lDestinatario.nuEmi}" nuDes="${lDestinatario.nuDes}"                                                                                                                                                                    coUbiDep="${lDestinatario.ccodDpto}" coUbiProv="${lDestinatario.ccodProv}" coUbiDist="${lDestinatario.ccodDist}" coServicio="${lDestinatario.coServicio}"/></td>                                            <td width="2%">${lDestinatario.fila}</td>                                            <td width="21%">${lDestinatario.deDependencia}</td>                                            <td width="15%">${lDestinatario.documento}<br/>${lDestinatario.nuDoc}</td>                                            <td width="20%">${lDestinatario.destinatario}</td>                                            <td width="15%"> ${lDestinatario.departamento}</td>                                            <td width="15%"> ${lDestinatario.direccion}</td>                                            <td width="5%"> ${lDestinatario.tiZona}</td>                                            <!-- VMBP - [INICIO - 15/08/2019] - Agregar prioridad -->                                            <td width="5%"> ${lDestinatario.dePrioridad}</td>                                            <!-- VMBP - [FIN - 15/08/2019] - Agregar prioridad -->                                        </tr>                                    </c:forEach>                                                                       </tbody>                            </table>                        </div>                                        </div>                                                    </td>            </tr>        </table></jsp:root>