<jsp:root xmlns:tiles="http://tiles.apache.org/tags-tiles" version="2.0" xmlns:c="http://java.sun.com/jsp/jstl/core" xmlns:jsp="http://java.sun.com/JSP/Page" xmlns:spring="http://www.springframework.org/tags" xmlns:fn="http://java.sun.com/jsp/jstl/functions" xmlns:form="http://www.springframework.org/tags/form">    <jsp:directive.page contentType="text/html;charset=UTF-8"/>    <jsp:output omit-xml-declaration="yes"/>    <script>        $(document).ready(function() {            var title1 = "-1";            var title2 = "-2";            var vListado = $('#txtValo1').val();            if (vListado > 0){                $("#slcResponsableMensajeria option[value=" + title2 + "]").hide()                $('select option[value="-2"]').attr("selected", false);            }else{                $("#slcResponsableMensajeria option[value=" + title1 + "]").hide();                $("#slcResponsableMensajeria option[value=" + title2 + "]").show();                $('select option[value="-2"]').attr("selected", true);                fu_calcularFechaPlazo();            }        });    </script>        <spring:eval expression="@applicationProps['application.version']" var="applicationVersion" scope="request"/>    <c:set var="rutactx" value="${pageContext.request.contextPath}/${applicationVersion}" scope="request" />    <input id="txtValo1" name="txtValo1" value="${listResponsable.size()}" type="hidden"/>                    <select id="slcResponsableMensajeria" style="width: 100%;" onchange="fu_calcularFechaPlazo()" >        <option value="-1">Seleccione un responsable</option>        <option value="-2">Sin responsable</option>        <c:forEach var="respMens" items="${listResponsable}" varStatus="status">            <option value="${respMens.codigo}">${respMens.nombre}</option>                   </c:forEach>    </select>     </jsp:root>