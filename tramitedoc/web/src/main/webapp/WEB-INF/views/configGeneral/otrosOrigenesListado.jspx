<jsp:root xmlns:tiles="http://tiles.apache.org/tags-tiles" version="2.0" xmlns:c="http://java.sun.com/jsp/jstl/core"          xmlns:jsp="http://java.sun.com/JSP/Page" xmlns:spring="http://www.springframework.org/tags"          xmlns:form="http://www.springframework.org/tags/form">    <jsp:directive.page contentType="text/html;charset=UTF-8"/>    <jsp:output omit-xml-declaration="yes"/>        <div class="ui-datatable ui-datatable-scrollable">        <div class="ui-datatable-scrollable-header">            <table class="ui-state-default" style="width:100%;">                  <thead>                        <tr>                            <th width="30px">N. Doc</th>                            <th width="50px">Ap Paterno</th>                            <th width="50px">Ap Materno</th>                            <th width="50px">Nombres</th>                            <th width="300px">Raz√≥n Social</th>                            <th width="40px">Dep.</th>                            <th width="40px">Prov.</th>                            <th width="40px">Dist.</th>                            <th width="100px">Direc.</th>                            <th width="40px">Correo</th>                            <th width="40px">Telf.</th>                        </tr>                  </thead>            </table>        </div>        <div class="bx_sb ui-datatable-scrollable-body" style='height:500px;'>               <table class="ui-datatable-data" id="tblOtroOrigenDet" style="width:100%;">            <tbody>                <c:forEach var="origen" items="${otroOrigenList}" varStatus="status">                    <c:choose>                        <c:when test="${status.count % 2 == 0}">                            <c:set var="rowStyle" scope="page" value="ui-datatable-even"/>                        </c:when>                        <c:otherwise>                            <c:set var="rowStyle" scope="page" value="ui-datatable-odd"/>                        </c:otherwise>                    </c:choose>                    <tr class="${rowStyle}">                        <c:choose>                            <c:when test="${status.count eq 1}">                                <td width="30px"><input type="hidden" name="idOtroOri" value="${origen.coOtrOri}"/> ${origen.nuDocOtrOri}</td>                                <td width="50px">${origen.deApePatOtr}</td>                                <td width="50px">${origen.deApeMatOtr}</td>                                <td width="50px">${origen.deNomOtr}</td>                                <td width="300px">${origen.deRazSocOtr}</td>                                <td width="40px">${origen.noDep}</td>                                <td width="40px">${origen.noPro}</td>                                <td width="40px">${origen.noDis}</td>                                <td width="100px">${origen.deDirOtroOri}</td>                                <td width="40px">${origen.deEmail}</td>                                <td width="40px">${origen.deTelefo}</td>                            </c:when>                            <c:otherwise>                                <td width="30px"><input type="hidden" name="idOtroOri" value="${origen.coOtrOri}"/> ${origen.nuDocOtrOri}</td>                                <td width="50px">${origen.deApePatOtr}</td>                                <td width="50px">${origen.deApeMatOtr}</td>                                <td width="50px">${origen.deNomOtr}</td>                                <td width="300px">${origen.deRazSocOtr}</td>                                <td width="40px">${origen.noDep}</td>                                <td width="40px">${origen.noPro}</td>                                <td width="40px">${origen.noDis}</td>                                <td width="100px">${origen.deDirOtroOri}</td>                                <td width="40px">${origen.deEmail}</td>                                <td width="40px">${origen.deTelefo}</td>                            </c:otherwise>                        </c:choose>                    </tr>                </c:forEach>                <c:if test="${empty otroOrigenList}">                    <td>No se encuentran registros.</td>                </c:if>            </tbody>			            </table>        </div>        <c:if test="${not empty otroOrigenList}">            <div>Registros: ${otroOrigenList.size()}</div>        </c:if>          <c:if test="${not empty msjEmision}">            <div class="alert alert-warning alert-dismissable" style="margin-bottom:0px;width:50%">                <button type="button" class="close" data-dismiss="alert" aria-hidden="true">x</button>                <h4 style="margin-bottom:0px;">${msjEmision}</h4>            </div>                    </c:if>     </div>    <input type="hidden" id="codOrigen" />    <script>        fn_eventOtrosOrigenes();    </script></jsp:root>