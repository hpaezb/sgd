<jsp:root xmlns:tiles="http://tiles.apache.org/tags-tiles" version="2.0" xmlns:c="http://java.sun.com/jsp/jstl/core"          xmlns:jsp="http://java.sun.com/JSP/Page" xmlns:spring="http://www.springframework.org/tags"          xmlns:form="http://www.springframework.org/tags/form">    <jsp:directive.page contentType="text/html;charset=UTF-8"/>    <jsp:output omit-xml-declaration="yes"/>        <div class="ui-datatable ui-datatable-scrollable">        <div class="ui-datatable-scrollable-header">            <table class="ui-state-default" style="width:100%;">                  <thead>                        <tr>                            <th width="5%">N. Doc</th>                            <th width="15%">Ap. Paterno</th>                            <th width="13%">Ap. Materno</th>                            <th width="13%">Nombres</th>                            <th width="7%">Dep.</th>                            <th width="7%">Prov.</th>                            <th width="7%">Dist.</th>                                                        <th width="7%">Direc.</th>                                 <th width="7%">Correo</th>                                 <th width="13%">Telf.</th>                             </tr>                  </thead>            </table>        </div>        <div class="bx_sb ui-datatable-scrollable-body" style='height:500px;'>               <table class="ui-datatable-edit-data" id="tblCiudadanoDet" style="width:100%;">            <tbody>                <c:forEach var="ciudadano" items="${ciudadanoList}" varStatus="status">                    <c:choose>                        <c:when test="${status.count % 2 == 0}">                            <c:set var="rowStyle" scope="page" value="ui-datatable-even"/>                        </c:when>                        <c:otherwise>                            <c:set var="rowStyle" scope="page" value="ui-datatable-odd"/>                        </c:otherwise>                    </c:choose>                    <tr class="${rowStyle}">                        <c:choose>                            <c:when test="${status.count eq 1}">                                <td width="8%"><input type="hidden" name="hid_IdCiudadano" value="${ciudadano.nuLem}"/> ${ciudadano.nuLem}</td>                                <td width="15%">${ciudadano.deApp}</td>                                <td width="15%">${ciudadano.deApm}</td>                                <td width="15%">${ciudadano.deNom}</td>                                <td width="8%">${ciudadano.noDep}</td>                                <td width="8%">${ciudadano.noPrv}</td>                                <td width="8%">${ciudadano.noDis}</td>                                                                <td width="8%">${ciudadano.deDomicil}</td>                                     <td width="8%">${ciudadano.deTelefo}</td>                                     <td width="8%">${ciudadano.deEmail}</td>                                 </c:when>                            <c:otherwise>                                <td width="8%"><input type="hidden" name="hid_IdCiudadano" value="${ciudadano.nuLem}"/> ${ciudadano.nuLem}</td>                                <td width="15%">${ciudadano.deApp}</td>                                <td width="15%">${ciudadano.deApm}</td>                                <td width="15%">${ciudadano.deNom}</td>                                <td width="8%">${ciudadano.noDep}</td>                                <td width="8%">${ciudadano.noPrv}</td>                                <td width="8%">${ciudadano.noDis}</td>                                                                <td width="8%">${ciudadano.deDomicil}</td>                                     <td width="8%">${ciudadano.deTelefo}</td>                                     <td width="8%">${ciudadano.deEmail}</td>                             </c:otherwise>                        </c:choose>                    </tr>                </c:forEach>                <c:if test="${empty ciudadanoList}">                    <td>No se encuentran registros.</td>                </c:if>                                </tbody>			            </table>        </div>        <c:if test="${not empty ciudadanoList}">		<div>Registros: ${ciudadanoList.size()}</div>	</c:if>  	<c:if test="${not empty msjEmision}">		<div class="alert alert-warning alert-dismissable" style="margin-bottom:0px;width:50%">			<button type="button" class="close" data-dismiss="alert" aria-hidden="true">x</button>			<h4 style="margin-bottom:0px;">${msjEmision}</h4>		</div>            	</c:if>     </div>    <input type="hidden" id="codCiudadano" />    <script>        fn_eventCiudadano();    </script></jsp:root>