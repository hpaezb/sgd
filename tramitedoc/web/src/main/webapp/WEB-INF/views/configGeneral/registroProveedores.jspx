<jsp:root xmlns:tiles="http://tiles.apache.org/tags-tiles" version="2.0" xmlns:fn="http://java.sun.com/jsp/jstl/functions" xmlns:c="http://java.sun.com/jsp/jstl/core"          xmlns:jsp="http://java.sun.com/JSP/Page" xmlns:spring="http://www.springframework.org/tags"          xmlns:form="http://www.springframework.org/tags/form"><jsp:directive.page contentType="text/html;charset=UTF-8"/><jsp:output omit-xml-declaration="yes"/>    <c:set var="divId" value="divProveedorMain" scope="request"/>    <c:set var="posiciona" value="width:1000px; height:750px;" scope="request"/>    <tiles:insertTemplate template="/WEB-INF/templates/windowmodaldraggable.jspx" flush="true">        <tiles:putAttribute name="title" value="Registro de Persona Jurídica"/>        <tiles:putAttribute name="body">        <div id="divProveedorBody">            <fieldset>                <div class="btn-group btn-group-lg">                    <!--<button id="btn-nuevo" title="Nuevo proveedor" class="btn btn-primary btn-lg" onclick="fu_goNuevoProveedor();" type="button"><span class="glyphicon glyphicon-floppy-disk"></span> Nuevo proveedor</button>-->                    <button id="btn-nuevo" title="Nueva Persona Jurídica" class="btn btn-primary btn-lg" onclick="fu_goNuevoProveedor();" type="button"><span class="glyphicon glyphicon-floppy-disk"></span> Nueva Persona Jurídica</button>                    <!--<button id="btn-edit" title="Editar proveedor" class="btn btn-primary btn-lg" onclick="fu_goEditarProveedor();" type="button"><span class="glyphicon glyphicon-floppy-disk"></span> Editar proveedor</button>-->                    <button id="btn-edit" title="Editar Persona Jurídica" class="btn btn-primary btn-lg" onclick="fu_goEditarProveedor();" type="button"><span class="glyphicon glyphicon-floppy-disk"></span> Editar Persona Jurídica</button>                    <button id="btn-salir" title="Salir" value="Salir" class="btn btn-primary btn-lg" onclick="removeDomId('divProveedorMain');" type="button"><span class="glyphicon glyphicon-log-out"></span> Salir</button>                </div>            </fieldset>            <fieldset>                <legend>Configuración Búsqueda</legend>                <table>                    <tbody>                        <tr>                            <td>RAZON SOCIAL:</td>                            <td><input type="text" id="txtBusRazonSocial" style="width:500px;" onkeyup="fn_buscarProveedores(event, this);" onkeypress="return fu_onKeyPressFiltrarTecladoCadena(event, false, public_letras)" maxlength="100"/></td>                            <td><button id="btn-buscar" title="Buscar" class="btn btn-default btnPequenoSGD" onclick="fn_cargarBusqProveedores();" type="button"><span class="glyphicon glyphicon-search"></span> BUSCAR</button></td>                        </tr>                        <tr>                            <td>NRO RUC:</td>                            <td><input type="text" id="txtBusNroRuc" maxlength="11" onkeyup="fn_buscarProveedores(event, this);" /></td>                            <td></td>                        </tr>                    </tbody>                </table>            </fieldset>            <fieldset>                <div id="divListaProveedores">                    <jsp:directive.include file="registroProveedoresListado.jspx"/>                </div>            </fieldset>        </div>        <div id="divProveedorEdit"><jsp:text/></div>            </tiles:putAttribute>        <tiles:putAttribute name="footer">           <jsp:text/>        </tiles:putAttribute>            </tiles:insertTemplate></jsp:root>