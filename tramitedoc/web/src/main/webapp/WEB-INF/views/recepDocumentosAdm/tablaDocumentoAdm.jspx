<jsp:root xmlns:tiles="http://tiles.apache.org/tags-tiles" version="2.0" xmlns:c="http://java.sun.com/jsp/jstl/core" xmlns:jsp="http://java.sun.com/JSP/Page" xmlns:spring="http://www.springframework.org/tags" xmlns:form="http://www.springframework.org/tags/form">    <jsp:directive.page contentType="text/html;charset=UTF-8"/>    <jsp:output omit-xml-declaration="yes"/>    <jsp:scriptlet><![CDATA[response.setHeader("Cache-Control", "max-age=43200");]]></jsp:scriptlet>    <script languaje="javascript">jQuery(document).ready(function() {            fu_eventoTablaRecepDocAdm();        });</script>  	    <div class="ui-datatable-fixed">        <input type="hidden" id="txtpnuAnn" name="txtpnuAnn" value=""/>        <input type="hidden" id="txtpnuEmi" name="txtnuEmi" value=""/>        <input type="hidden" id="txtpnuDes" name="txtpnuDes" value=""/>        <input type="hidden" id="txtptiCap" name="txtptiCap" value=""/>        <input type="hidden" id="txtCoEstadoDoc" name="txtCoEstadoDoc" value=""/>        <input type="hidden" id="txtPaginaRecepcion" name="txtPaginaRecepcion" value="${vPagina}"/><!-- HPB 20/02/2020 - Requerimiento Paginación recepcionados -->        <table id ="myTableFixed" class="display" width="100%">            <thead>                <tr class="ui-state-default">                    <th style="display:none"></th>                     <th style="display:none"></th>                    <th style="display:none"></th>                    <th style="display:none"></th>                                                 <th style="display:none"></th>                                                 <th width="7%"></th>                    <th width="4%">N°</th>                    <th width="4%">F. EMISION</th>                    <th width="15%">REMITENTE</th>                    <th width="8%">TIPO DOC.</th>                    <th width="10%">N° DOCUMENTO</th>                    <th width="12%">DESTINATARIO</th>                    <th width="23%">ASUNTO</th>                    <th width="8%">PLAZO ATENCIÓN </th><!--[HPB-02/10/20] Inicio - Plazo de Atencion-->                                        <th width="10%">NRO EXPEDIENTE</th>                    <th width="7%">ESTADO</th>                    <th style="display:none"></th>                    <th style="display:none"></th><!--[HPB-02/10/20] Inicio - Plazo de Atencion-->                </tr>            </thead>				            <tbody>                <c:forEach var="recepDocumAdm" items="${recepDocumAdmList}" varStatus="status">                    <tr class="gradeA" tabindex="0">                        <td style="display:none">${status.count}</td>                        <td style="display:none">${recepDocumAdm.nuAnn}</td>                        <td style="display:none">${recepDocumAdm.nuEmi}</td>                        <td style="display:none">${recepDocumAdm.tiCap}</td>                                                            <td style="display:none">${recepDocumAdm.nuDes}</td>                                                            <td>                            <c:choose>                                <c:when test='${recepDocumAdm.esDocRec eq "0"}'>                                    <c:choose>                                        <c:when test='${recepDocumAdm.coPri eq "3"}'>                                            <button title ="Editar Documento muy Urgente" class="btn btn-danger btn-xs" onclick="editarDocumentoRecepClick('${recepDocumAdm.nuAnn}', '${recepDocumAdm.nuEmi}','${recepDocumAdm.nuDes}','${recepDocumAdm.existeDoc}','${recepDocumAdm.existeAnexo}');" type="button"><span class="glyphicon glyphicon-edit"><jsp:text/></span></button>                                                                            </c:when>                                        <c:when test='${recepDocumAdm.coPri eq "2"}'>                                            <button title ="Editar Documento Urgente" class="btn btn-warning btn-xs" onclick="editarDocumentoRecepClick('${recepDocumAdm.nuAnn}', '${recepDocumAdm.nuEmi}','${recepDocumAdm.nuDes}','${recepDocumAdm.existeDoc}','${recepDocumAdm.existeAnexo}');" type="button"><span class="glyphicon glyphicon-edit"><jsp:text/></span></button>                                                                            </c:when>                                        <c:otherwise>                                            <button title ="Editar Documento" class="btn btn-success btn-xs" onclick="editarDocumentoRecepClick('${recepDocumAdm.nuAnn}', '${recepDocumAdm.nuEmi}','${recepDocumAdm.nuDes}','${recepDocumAdm.existeDoc}','${recepDocumAdm.existeAnexo}');" type="button"><span class="glyphicon glyphicon-edit"><jsp:text/></span></button>                                                                            </c:otherwise>                                    </c:choose>                                </c:when>                                <c:otherwise>                                    <button title ="Editar Documento" class="btn btn-default btn-xs" onclick="editarDocumentoRecepClick('${recepDocumAdm.nuAnn}', '${recepDocumAdm.nuEmi}','${recepDocumAdm.nuDes}','${recepDocumAdm.existeDoc}','${recepDocumAdm.existeAnexo}');" type="button"><span class="glyphicon glyphicon-edit"><jsp:text/></span></button>                                                                    </c:otherwise>                            </c:choose>                            <c:choose>                                <c:when test='${recepDocumAdm.existeDoc eq "1"}'>                                    <button title ="Ver Documento" class="btn btn-default btn-xs" onclick="fn_verDocumento('${recepDocumAdm.nuAnn}', '${recepDocumAdm.nuEmi}', 'M020102', '${recepDocumAdm.nuDes}');" type="button"><span class="glyphicon glyphicon-file"><jsp:text/></span></button>                                </c:when>                            </c:choose>									                            <c:choose>                                <c:when test='${recepDocumAdm.existeAnexo eq "1"}'>                                    <button title ="Documentos Anexos" class="btn btn-default btn-xs" onclick="fn_verAnexo('${recepDocumAdm.nuAnn}', '${recepDocumAdm.nuEmi}', '${recepDocumAdm.nuDes}');" type="button"><span class="glyphicon glyphicon-paperclip"><jsp:text/></span></button>                                </c:when>                            </c:choose>	                         </td>                         <!--<c:choose>                            <c:when test='${recepDocumAdm.coPri eq "3"}'>                                <td align="center" style="background-color: #D9534F; color:white; font-size: 1em;">                                    ${recepDocumAdm.nuCorDes}                                </td>                            </c:when>                        </c:choose>                        <c:choose>                            <c:when test='${recepDocumAdm.coPri eq "2"}'>                                <td align="center" style="background-color: #F0AD4E;color:white;font-size: 1em;">                                    ${recepDocumAdm.nuCorDes}                                </td>                            </c:when>                        </c:choose>                        <c:choose>                            <c:when test='${recepDocumAdm.coPri eq "1"}'>-->                                <td align="center">${recepDocumAdm.nuCorDes}</td>                            <!--</c:when>                            </c:choose>-->                        <td>${recepDocumAdm.feEmiCorta}</td>                        <td>${recepDocumAdm.deOriEmi}</td>                        <td>${recepDocumAdm.deTipDocAdm}</td>                        <td>${recepDocumAdm.nuDoc}</td>                        <td>${recepDocumAdm.deEmpDes}</td>                        <td>${recepDocumAdm.deAsuM}</td>                        <td align="center">${recepDocumAdm.fePlaAte}</td><!--[HPB-02/10/20] Inicio - Plazo de Atencion-->                        <td>${recepDocumAdm.nuExpediente}</td>                        <td>${recepDocumAdm.coEtiquetaRec}_${recepDocumAdm.coAvanceRec}_${recepDocumAdm.deEsDocDes}</td><!--HPB 18/09/2019 Avance de recepcionados-->                        <td style="display:none">${recepDocumAdm.esDocRec}</td>                        <td style="display:none">${recepDocumAdm.coEstPla}</td><!--[HPB-02/10/20] Inicio - Plazo de Atencion-->                                            </tr>                </c:forEach>            </tbody>			        </table>    </div>    <c:if test="${not empty msjEmision}">            <div class="alert alert-warning alert-dismissable" style="margin-bottom:0px;width:50%">                <button type="button" class="close" data-dismiss="alert" aria-hidden="true">x</button>                <h4 style="margin-bottom:0px;">${msjEmision}</h4>            </div>      </c:if>          <div id="divflotante" class="divTip">&amp;nbsp;</div>           <!-- HPB 20/02/2020 - Inicio - Requerimiento Paginación recepcionados -->        <fieldset>            <table width="100%">                <tr>                    <td width="30%">                        <b>Total de Registros : ${cantRegistros}</b>                    </td>                    <td width="40%">                        <table width="100%" style="color: #004370;">                            <tr>                                <td align="right">                                    <c:choose>                                        <c:when test='${vPagina eq "1"}'>                                            <button id="btn-ReemplazarDoc" title="Ir primera página" class="btn btn-default btn-xs" onclick="fn_paginationChangeTipoBusqRecepDocuAdmPreviusFirts();" type="button" disabled="true">                                                <span class="glyphicon glyphicon-fast-backward"><jsp:text/></span>                                            </button>                                                                                         <button id="btn-ReemplazarDoc" title="Página anterior" class="btn btn-default btn-xs" onclick="fn_paginationChangeTipoBusqRecepDocuAdmPrevius();" type="button" disabled="true">                                                <span class="glyphicon glyphicon-backward"><jsp:text/></span>                                            </button>                                                                                           </c:when>                                        <c:otherwise>                                            <button id="btn-ReemplazarDoc" title="Ir primera página" class="btn btn-default btn-xs" onclick="fn_paginationChangeTipoBusqRecepDocuAdmPreviusFirts();" type="button">                                                <span class="glyphicon glyphicon-fast-backward"><jsp:text/></span>                                            </button>                                                                                         <button id="btn-ReemplazarDoc" title="Página anterior" class="btn btn-default btn-xs" onclick="fn_paginationChangeTipoBusqRecepDocuAdmPrevius();" type="button">                                                <span class="glyphicon glyphicon-backward"><jsp:text/></span>                                            </button>                                                                                   </c:otherwise>                                    </c:choose>                                                                                                       </td>                                <td align="center">                                    <b>Página : </b>                                    <c:choose>                                        <c:when test='${vTotalPaginas eq "1"}'>                                            <select class="form-control" id="cantPaginasRecep" name ="cantPaginasRecep" onchange="fn_changePaginaRecepDocuAdm();">                                                <option Value="1">1</option><!--HPB 25/10/19-->                                            </select>                                               <b> de 1</b>                                        </c:when>                                        <c:otherwise>                                            <select class="form-control" id="cantPaginasRecep" name ="cantPaginasRecep" onchange="fn_changePaginaRecepDocuAdm();">                                                <c:forEach var="listPaginas" items="${lsCantidadPg}" varStatus="status">                                                    <option value="${listPaginas}">${listPaginas}</option>                                                </c:forEach>                                            </select>                                                                                                                <b> de ${vTotalPaginas}</b>                                                                                </c:otherwise>                                    </c:choose>                                </td>                                <td align="left">                                    <c:choose>                                        <c:when test='${vPagina eq vTotalPaginas}'>                                            <button id="btn-ReemplazarDoc" title="Página siguiente" class="btn btn-default btn-xs" onclick="fn_paginationChangeTipoBusqRecepDocuAdmNext();" type="button" disabled="true">                                                <span class="glyphicon glyphicon-forward"><jsp:text/></span>                                            </button>                                               <button id="btn-ReemplazarDoc" title="Ir última página" class="btn btn-default btn-xs" onclick="fn_paginationChangeTipoBusqRecepDocuAdmNextLast();" type="button" disabled="true">                                                <span class="glyphicon glyphicon-fast-forward"><jsp:text/></span>                                            </button>                                                                                            </c:when>                                        <c:otherwise>                                            <button id="btn-ReemplazarDoc" title="Página siguiente" class="btn btn-default btn-xs" onclick="fn_paginationChangeTipoBusqRecepDocuAdmNext();" type="button">                                                <span class="glyphicon glyphicon-forward"><jsp:text/></span>                                            </button>                                               <button id="btn-ReemplazarDoc" title="Ir última página" class="btn btn-default btn-xs" onclick="fn_paginationChangeTipoBusqRecepDocuAdmNextLast();" type="button">                                                <span class="glyphicon glyphicon-fast-forward"><jsp:text/></span>                                            </button>                                                                                        </c:otherwise>                                    </c:choose>                                                               </td>                                                                   </tr>                        </table>                                        </td>                    <td width="30%">                                        </td>                </tr>                                            </table>        </fieldset>        <!-- HPB 20/02/2020 - Fin - Requerimiento Paginación recepcionados -->                                        <script>        $(document).ready(function(){            var idTable="myTableFixed";            var col=15;            setEtiquetasListaDoc(idTable,col);              /*HPB 20/02/2020 - Inicio - Requerimiento Paginación recepcionados*/            var pag = jQuery('#txtPaginaRecepcion').val();            $('#cantPaginasRecep').find('option[value="'+pag+'"]').prop('selected', true);             jQuery('#txtPaginaRecepcion').val("");            /*HPB 20/02/2020 - Fin - Requerimiento Paginación recepcionados*/            pintarEstadosPlazoAtencionRecep();/*HPB-02/10/20] Inicio - Plazo de Atencion*/        });    </script></jsp:root>	