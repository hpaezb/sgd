<jsp:root xmlns:tiles="http://tiles.apache.org/tags-tiles" version="2.0" xmlns:c="http://java.sun.com/jsp/jstl/core" xmlns:jsp="http://java.sun.com/JSP/Page" xmlns:spring="http://www.springframework.org/tags" xmlns:form="http://www.springframework.org/tags/form">    <jsp:directive.page contentType="text/html;charset=UTF-8"/>    <jsp:output omit-xml-declaration="yes"/>    <a href="#" class="list-group-item active" onclick="fn_refreshCheckWebSocket('${BandEntrada.coDep}');refrescarBandejaEntrada();" title="Actualizar Cambios"><!--[HPB] 30/10/20 Refrescar check verde-->        <h4 class="list-group-item-heading" style="text-align: center;font-size:21px" ><span class="glyphicon glyphicon-refresh"><jsp:text/></span> Documentos de ${siglasDep}</h4>        <p class="list-group-item-text">Resumen de los documentos principales de <strong>${siglasDep}</strong>.</p>    </a>    <c:forEach var="BandEntrada" items="${bandejaEntradaList}" varStatus="status">        <a href="#" class="list-group-item" onclick="preloadMostrarVentana('${BandEntrada.tiPen}', '${BandEntrada.coDep}', '${BandEntrada.coBandeja}');">            <span class="badge">${BandEntrada.nuCan}</span>            <h4 class="list-group-item-heading">${BandEntrada.dePen}</h4>            <p class="list-group-item-text">${BandEntrada.deResumen}</p>        </a>    </c:forEach>    <c:forEach var="etiquetas" items="${etiquetasList}" varStatus="status">        <c:set var="auxIco" value="glyphicon glyphicon-tag"/>        <a href="#" class="list-group-item" onclick="mostrarListaEtiquetas('${etiquetas.coEst}','${etiquetas.coDepDes}')">            <span class="badge">${etiquetas.numeroDocumentos}</span>            <h6 class="list-group-item-heading" style="font-size: 18px;font-style: italic;"><span class="${auxIco}"><jsp:text/><input type="hidden" name="icoEtiqueta" value="${etiquetas.coEst}" /></span> ${etiquetas.descripcion}</h6>        </a>    </c:forEach>    <script>        $(document).ready(function(){            mostrarEtiquetas();          });    </script></jsp:root>      